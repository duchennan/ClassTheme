 * {
    margin: 0;
    padding: 0;
    }
h1{
   color: red;
   text-align: center;
   font-size: 50px;
   }
nav{
    text-align: center;
	}
a{
  text-decoration: none;
  border: solid;
  border-radius: 25px;
  }        
div{
	position: relative;
	}
.w1{
    position: absolute; 
	top: 600px; 
	left: 50px;
	font-size: 3em;
	color: black;
	}
img{
    width: 100%;
	height:100%;
	}
.w2{
	position: absolute; 
	top: 300px; 
	left: 1300px;
	font-size: 3em;
	color: black;
	}
.w1:hover{
          transition: all 0.9s;
		  cursor:pointer;
		  font-size: 70px; 
		  color: red;
		  }
.w2:hover{
          transition: all 0.9s;  
	      cursor:pointer;
		  font-size: 70px; 
		  color: blue;
		  }
div img:hover {
               transition: all 0.8s;
               transform: scale(1.1);
               }
input{
      border: solid 1px red;
	  color: red;
	  }
